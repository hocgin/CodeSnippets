	<string>import WidgetKit
import SwiftUI
import SharedAit
import AppIntents

enum &lt;#name#&gt; {
    
}
// MARK: - Data
extension &lt;#name#&gt; {
    struct DataEntry: TimelineEntry {
        var date: Date
        let configuration: DataConfiguration
    }
    struct EntryView : View {
        var entry: DataProvider.Entry

        var body: some View {
            Text("View")
        }
    }
    struct DataConfiguration: WidgetConfigurationIntent {
        public static var persistentIdentifier: String {
            "in.hocg.app.widget.&lt;#name#&gt;"
        }
        static var title: LocalizedStringResource { "Configuration" }

        // An example configurable parameter.
        @Parameter(title: "Favorite Emoji", default: "ðŸ˜ƒ")
        var favoriteEmoji: String
    }
    
    struct DataProvider: AppIntentTimelineProvider {
        func placeholder(in context: Context) -&gt; DataEntry {
            DataEntry(date: Date(), configuration: DataConfiguration())
        }

        func snapshot(for configuration: DataConfiguration, in context: Context) async -&gt; DataEntry {
            DataEntry(date: Date(), configuration: configuration)
        }
        
        func timeline(for configuration: DataConfiguration, in context: Context) async -&gt; Timeline&lt;DataEntry&gt; {
            var entries: [DataEntry] = []

            // Generate a timeline consisting of five entries an hour apart, starting from the current date.
            let currentDate = Date()
            for hourOffset in 0 ..&lt; 5 {
                let entryDate = Calendar.current.date(
                    byAdding: .hour,
                    value: hourOffset,
                    to: currentDate
                )!
                let entry = DataEntry(
                    date: entryDate,
                    configuration: configuration
                )
                entries.append(entry)
            }

            return Timeline(entries: entries, policy: .atEnd)
        }

        //    func relevances() async -&gt; WidgetRelevances&lt;ConfigurationAppIntent&gt; {
        //        // Generate a list containing the contexts this widget is relevant in.
        //    }
    }
}
// MARK: - Mock
extension &lt;#name#&gt;.DataConfiguration {
    static var mock: Self {
        let intent = Self()
        intent.favoriteEmoji = "ðŸ˜€"
        return intent
    }
}

